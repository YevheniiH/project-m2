<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <container name="container-1" htmlTag="div" htmlClass="container1"/>
            <container name="container-2" htmlTag="div" htmlClass="container2">
                <container name="container-3" htmlTag="div" htmlClass="container3"/>
            </container>
            <container name="container-4" htmlTag="div" htmlClass="container4"/>
        </referenceContainer>
        <referenceContainer name="custom-sidebar">
            <block class="Magento\Framework\View\Element\Template" name="sidebar-right-block" template="Magento_Theme::2-columns-right-layout.phtml">
            </block>
        </referenceContainer>
        <referenceContainer name="container-3">
            <block class="Magento\Framework\View\Element\Template" name="block-test111" template="Magento_Theme::block111.phtml">
            </block>
            <block class="Magento\Framework\View\Element\Template" name="block-test222" template="Magento_Theme::block222.phtml">
            </block>
        </referenceContainer>
        <referenceContainer name="container-4">
            <block class="Magento\Framework\View\Element\Template" name="block-test11" template="Magento_Theme::block1333.phtml">
            </block>
            <block class="Magento\Framework\View\Element\Template" name="block-test2" template="Magento_Theme::block2444.phtml">
            </block>
        </referenceContainer>
        <move element="block-test2" destination="container-3" before="block-test222"/>
        <move element="container-1" destination="container-4" before="-"/>
        <move element="container-4" destination="content" before="-"/>
        <move element="block-test11" destination="container-1" before="-"/>
        <referenceContainer name="container-1" remove="true"/>
        <!-- Address link -->
        <referenceBlock name="top.links" remove="true"/>

    </body>
</page>
